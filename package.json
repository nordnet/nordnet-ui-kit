{
  "name": "nordnet-ui-kit",
  "version": "0.0.0-development",
  "description": "Nordnets React powered UI kit",
  "main": "dist/cjs",
  "module": "dist/esm",
  "sideEffects": false,
  "scripts": {
    "precommit": "lint-staged",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "clean": "rimraf dist",
    "dev": "run-s start",
    "build": "run-s clean babel:*",
    "babel:esm": "BABEL_ENV=esm babel -d dist/esm src/",
    "babel:cjs": "BABEL_ENV=cjs babel -d dist/cjs src/",
    "watch": "npm run babel -- --watch",
    "babel": "babel src -d dist --copy-files --ignore .md",
    "lint": "eslint --ext=.jsx --ext=.js src test",
    "test": "BABEL_ENV=cjs mocha --opts mocha.opts",
    "tdd": "npm test -- --watch",
    "coverage": "cross-env NODE_ENV=coverage nyc npm test",
    "prepush": "run-p lint test",
    "prepublish": "in-publish && run-s lint test build || not-in-publish",
    "start": "BABEL_ENV=cjs styleguidist server --colors",
    "build:docs": "rimraf documentation/build && styleguidist build",
    "predocs-publish": "run-s build:docs",
    "docs-publish": "gh-pages -d documentation/dist --repo https://$GH_TOKEN@github.com/nordnet/nordnet-ui-kit.git --silent",
    "precoveralls": "run-s coverage",
    "coveralls": "BABEL_ENV=cjs coveralls < coverage/lcov.info",
    "semantic-release": "semantic-release",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "format": "prettier --write '{src,test}/**/*.{js,jsx}'"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "nordnet",
    "react",
    "ui",
    "kit"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "nyc": {
    "extension": [
      ".js",
      ".jsx"
    ],
    "all": true,
    "instrument": false,
    "sourceMap": false,
    "reporter": [
      "lcov",
      "text-summary",
      "text"
    ]
  },
  "author": "Gustaf Zetterlund <gustaf.zetterlund@nordnet.se>",
  "repository": "nordnet/nordnet-ui-kit",
  "bugs": {
    "url": "https://github.com/nordnet/nordnet-ui-kit/issues"
  },
  "homepage": "https://nordnet.github.io/nordnet-ui-kit",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.3",
    "color": "^1.0.3",
    "lodash": "^4.17.10",
    "react-transition-group": "^2.5.2"
  },
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=15",
    "react-dom": ">=15",
    "react-jss": ">=7"
  },
  "devDependencies": {
    "@nordnet/prettier-config": "^2.0.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "commitizen": "^3.0.5",
    "coveralls": "^3.0.2",
    "cross-env": "^5.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^5.2.1",
    "eslint-plugin-react": "^6.10.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "in-publish": "^2.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^13.2.0",
    "lint-staged": "^8.1.1",
    "lodash.camelcase": "^4.3.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-jss": "^8.4.0",
    "react-router-dom": "^4.2.2",
    "react-styleguidist": "^5.5.10",
    "react-test-renderer": "^15.6.2",
    "rimraf": "^2.6.1",
    "semantic-release": "^12.4.1",
    "sinon": "^7.2.3",
    "validate-commit-msg": "^2.12.1",
    "webpack": "2.2.1",
    "webpack-node-externals": "^1.5.4"
  }
}

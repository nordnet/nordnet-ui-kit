{
  "name": "nordnet-ui-kit",
  "version": "0.2.0-beta.9",
  "description": "Nordnets React powered UI kit",
  "main": "dist",
  "scripts": {
    "clean": "rimraf dist",
    "build": "run-s clean babel",
    "watch": "npm run babel -- --watch",
    "babel": "babel src -d dist --copy-files --ignore .md",
    "lint": "eslint --ext=.jsx --ext=.js src test",
    "test": "mocha-webpack --opts test/mocha.webpack.opts",
    "tdd": "npm test -- --watch",
    "prepush": "run-p lint test",
    "prepublish": "in-publish && run-s lint test build || not-in-publish",
    "postpublish": "run-s clean && git push --follow-tags --no-verify && run-s docs-publish",
    "start": "styleguidist server --colors",
    "build:docs": "rimraf documentation/build && styleguidist build",
    "predocs-publish": "run-s build:docs",
    "docs-publish": "gh-pages -d documentation/dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "nordnet",
    "react",
    "ui",
    "kit"
  ],
  "author": "Gustaf Zetterlund <gustaf.zetterlund@nordnet.se>",
  "repository": "nordnet/nordnet-ui-kit",
  "bugs": {
    "url": "https://github.com/nordnet/nordnet-ui-kit/issues"
  },
  "homepage": "https://nordnet.github.io/nordnet-ui-kit",
  "license": "MIT",
  "peerDependencies": {
    "react": "15.x",
    "react-dom": "15.x"
  },
  "dependencies": {
    "array-equal": "^1.0.0",
    "bezier-easing": "^2.0.3",
    "classnames": "^2.2.3",
    "color": "^1.0.3",
    "d3-interpolate": "^1.0.0",
    "lodash": "^4.17.4",
    "react-motion-ui-pack": "^0.10.2",
    "react-prop-types": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "gh-pages": "^0.12.0",
    "in-publish": "^2.0.0",
    "jsdom": "^9.11.0",
    "jss": "^7.0.3",
    "jss-preset-default": "^2.0.0",
    "jss-theme-reactor": "^0.11.1",
    "lodash.camelcase": "^4.3.0",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-docgen": "^2.13.0",
    "react-dom": "^15.4.2",
    "react-styleguidist": "^5.0.6",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "webpack": "2.2.1",
    "webpack-node-externals": "^1.5.4"
  }
}

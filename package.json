{
  "name": "nordnet-ui-kit",
  "version": "0.0.8",
  "description": "Nordnets React powered UI kit",
  "main": "dist/nordnet-ui-kit.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm-run-all clean webpack:bundle webpack:individual",
    "webpack:bundle": "NODE_ENV=production webpack --type bundle --progress --config webpack.config.js",
    "webpack:individual": "NODE_ENV=production webpack --type individual --progress --config webpack.config.js",
    "test": "mocha",
    "lint": "eslint --ext=.jsx --ext=.js src test",
    "prepush": "npm-run-all lint test",
    "prepublish": "npm-run-all lint test build",
    "postpublish": "npm run clean && git push --follow-tags"
  },
  "keywords": [
    "nordnet",
    "react",
    "ui",
    "kit"
  ],
  "author": "Gustaf Zetterlund <gustaf.zetterlund@nordnet.se>",
  "repository": "nordnet/nordnet-ui-kit",
  "bugs": {
    "url": "https://github.com/nordnet/nordnet-ui-kit/issues"
  },
  "homepage": "https://github.com/nordnet/nordnet-ui-kit#readme",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.3",
    "lodash.kebabcase": "^4.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-motion": "^0.4.1",
    "react-prop-types": "^0.3.0",
    "react-pure-render": "^1.0.2",
    "react-svg-sprite-icon": "0.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^1.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-config-nordnet": "^1.1.1",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "husky": "^0.10.2",
    "minimist": "^1.2.0",
    "mocha": "^2.3.4",
    "npm-run-all": "^1.5.0",
    "postcss-loader": "^0.8.0",
    "react-addons-test-utils": "^0.14.0",
    "rimraf": "^2.5.1",
    "sass-loader": "^3.1.2",
    "sass-variable-loader": "^0.0.3",
    "style-loader": "^0.13.0",
    "svg-icon-template-loader": "0.0.4",
    "webpack": "^1.12.13"
  }
}

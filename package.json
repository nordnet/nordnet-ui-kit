{
  "name": "nordnet-ui-kit",
  "version": "0.0.1",
  "description": "Nordnets React powered UI kit",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel ./src -d ./dist",
    "test": "mocha",
    "lint": "eslint --ext=.jsx --ext=.js ./src",
    "prepush": "npm-run-all lint test",
    "prepublish": "npm-run-all lint test build",
    "postpublish": "npm run clean && git push --follow-tags"
  },
  "keywords": [
    "nordnet",
    "react",
    "ui",
    "kit"
  ],
  "author": "Gustaf Zetterlund <gustaf.zetterlund@nordnet.se>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-prop-types": "^0.3.0",
    "react-pure-render": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "enzyme": "^1.4.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-config-nordnet": "^1.1.1",
    "eslint-plugin-react": "^3.16.1",
    "husky": "^0.10.2",
    "mocha": "^2.3.4",
    "npm-run-all": "^1.5.0",
    "react-addons-test-utils": "^0.14.6"
  }
}

{
  "name": "nordnet-ui-kit",
  "version": "0.2.0-beta.9",
  "description": "Nordnets React powered UI kit",
  "main": "dist",
  "scripts": {
    "clean": "rimraf dist",
    "build": "run-s clean babel",
    "watch": "npm run babel -- --watch",
    "babel": "babel src -d dist --copy-files --ignore .md",
    "lint": "eslint --ext=.jsx --ext=.js src test",
    "test": "__USE_REM__=true mocha-webpack --opts test/mocha.webpack.opts",
    "tdd": "npm test -- --watch",
    "prepush": "run-p lint test",
    "prepublish": "run-s lint test build",
    "postpublish": "run-s clean && git push --follow-tags --no-verify && run-s docs-publish",
    "start": "styleguidist server --colors",
    "build:docs": "rimraf documentation/build && styleguidist build",
    "predocs-publish": "run-s build:docs",
    "docs-publish": "git-directory-deploy --directory ./documentation/dist --branch gh-pages"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "nordnet",
    "react",
    "ui",
    "kit"
  ],
  "author": "Gustaf Zetterlund <gustaf.zetterlund@nordnet.se>",
  "repository": "nordnet/nordnet-ui-kit",
  "bugs": {
    "url": "https://github.com/nordnet/nordnet-ui-kit/issues"
  },
  "homepage": "https://nordnet.github.io/nordnet-ui-kit",
  "license": "MIT",
  "peerDependencies": {
    "react": "15.x",
    "react-dom": "15.x"
  },
  "dependencies": {
    "array-equal": "^1.0.0",
    "bezier-easing": "^2.0.3",
    "classnames": "^2.2.3",
    "color": "^1.0.3",
    "d3-interpolate": "^1.0.0",
    "lodash": "^4.17.4",
    "react-addons-shallow-compare": "^15.1.0",
    "react-motion-ui-pack": "^0.10.2",
    "react-prop-types": "^0.4.0",
    "react-select": "^1.0.0-rc.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.27.3",
    "cssnano": "^3.10.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "git-directory-deploy": "^1.5.0",
    "jsdom": "^9.11.0",
    "jss": "^6.5.0",
    "jss-preset-default": "^1.3.1",
    "jss-theme-reactor": "^0.10.0",
    "lodash-webpack-plugin": "^0.11.2",
    "lodash.camelcase": "^4.3.0",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "null-loader": "^0.1.1",
    "postcss-loader": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-docgen": "^2.13.0",
    "react-dom": "^15.4.2",
    "react-styleguidist": "^5.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.1",
    "sass-variable-loader": "^0.1.1",
    "sinon": "^2.1.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "svg-icon-template-loader": "0.0.6",
    "webpack": "2.2.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-node-externals": "^1.2.0"
  }
}

{
  "env": {
    "cjs": {
      "presets": [
        ["env", {
          "targets": {
            "node": "current",
            "browsers": ["last 2 versions"]
          }
        }],
        "react"
      ],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    },
    "esm": {
      "presets": [
        ["env", {
          "modules": false,
          "targets": {
            "node": "current",
            "browsers": ["last 2 versions"]
          }
        }],
        "react"
      ],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    }
  }
}
